{
  "name": "spotify-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "babel": "npx babel src --out-dir babel-out --presets react-app/prod",
    "babelw": "npx babel --watch src --out-dir babel-out --presets react-app/prod",
    "bundle": "browserify -d babel-out/main.js -o public/js/spotify-frontend-bundle.js",
    "bundlew": "watchify babel-out/main.js -o public/js/spotify-frontend-bundle.js --debug --verbose",
    "build": "npm run babel && npm run bundle",
    "build:watch": "watch \"npm run build\" src"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "file-saver": "^2.0.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  }
}
